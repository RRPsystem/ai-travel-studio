{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "https://ai-travelstudio.vercel.app",
      "permanent": false,
      "has": [
        {
          "type": "host",
          "value": "travelbro.nl"
        }
      ]
    },
    {
      "source": "/",
      "destination": "https://ai-travelstudio.vercel.app",
      "permanent": false,
      "has": [
        {
          "type": "host",
          "value": "www.travelbro.nl"
        }
      ]
    }
  ],
  "crons": [
    {
      "path": "/api/cron/process-scheduled-messages",
      "schedule": "0 * * * *"
    }
  ]
}
